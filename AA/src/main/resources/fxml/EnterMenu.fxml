<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Sphere?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<BorderPane prefHeight="600" prefWidth="800" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="view.menu.EnterMenu">
    <left>
        <Sphere drawMode="LINE" layoutX="219.0" layoutY="464.0" radius="75"/>
    </left>
    <right>
        <Sphere drawMode="LINE" layoutX="219.0" layoutY="464.0" radius="75"/>
    </right>
    <center>
        <VBox alignment="CENTER" fillWidth="false" prefHeight="403.0" prefWidth="600.0" spacing="10">
            <Text fill="#282727" strokeType="OUTSIDE" strokeWidth="0.0" text="AA" wrappingWidth="130.39993286132812">
                <font>
                    <Font name="Modern No. 20" size="92.0"/>
                </font>
            </Text>
            <Button layoutX="211.0" layoutY="168.0" mnemonicParsing="false" onMouseClicked="#enterRegisterMenu"
                    prefHeight="30.0" prefWidth="454.0" text="Register">
            </Button>
            <Button id="loginButton" layoutX="211.0" layoutY="131.0" mnemonicParsing="false"
                    onMouseClicked="#enterLoginMenu" prefHeight="26.0" prefWidth="454.0" text="Login">
            </Button>
            <Button layoutX="211.0" layoutY="210.0" mnemonicParsing="false" onMouseClicked="#enterMainMenuAsGuest"
                    prefHeight="26.0" prefWidth="454.0" text="Guest">
            </Button>
            <Button layoutX="211.0" layoutY="210.0" mnemonicParsing="false" onMouseClicked="#exit"
                    prefHeight="26.0" prefWidth="454.0" text="Exit">
            </Button>
        </VBox>
    </center>
</BorderPane>
