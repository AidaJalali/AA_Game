            <Button layoutX="149.0" layoutY="252.0" mnemonicParsing="false" onMouseClicked="#enterSettingMenu" prefHeight="26.0" prefWidth="588.0" text="SettingMenu" />
            <Button layoutX="149.0" layoutY="167.0" mnemonicParsing="false" onMouseClicked="#enterProfileMenu" prefHeight="26.0" prefWidth="588.0" text="ProfileMenu" />
