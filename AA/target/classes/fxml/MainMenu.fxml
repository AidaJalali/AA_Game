        <Sphere drawMode="LINE" layoutX="219.0" layoutY="464.0" radius="75" />
    </left>
    <right>
        <Sphere drawMode="LINE" layoutX="219.0" layoutY="464.0" radius="75" />
    </right>

    <center>
        <VBox alignment="CENTER" fillWidth="false" prefHeight="403.0" prefWidth="600.0" spacing="10">
         <Rectangle fx:id="profile" arcHeight="5.0" arcWidth="5.0" fill="#feffff" height="118.0" stroke="BLACK" strokeType="INSIDE" width="144.0" />
